    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-configuration": "^2.0.0",
    "aurelia-dialog": "^2.0.0-rc.3",
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "bootstrap": "^4.1.1",
    "bootstrap-material-design": "^4.1.1",
    "es6-promisify": "^6.0.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "3.2.1",
    "moment-timezone": "^0.5.23",
    "momentjs": "^2.0.0",
    "node-waves": "^0.7.6",
    "numeral": "^2.0.6",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.6",
    "snackbarjs": "^1.1.0",
    "tslint": "^5.12.0"
  }
}
